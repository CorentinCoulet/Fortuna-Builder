generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Actions {
  id          Int     @id @default(autoincrement())
  definition  Json
  description Json
  version     String
}

model EquipmentItemTypes {
  id          Int     @id @default(autoincrement())
  definition  Json
  title       Json
  version     String
}

model ItemTypes {
  id          Int     @id @default(autoincrement())
  definition  Json
  title       Json
  version     String
}

model Items {
  id          Int     @id @default(autoincrement())
  definition  Json
  title       Json
  version     String
}

model JobItems {
  id          Int     @id @default(autoincrement())
  definition  Json
  title       Json
  description Json
  version     String
}

model States {
  id          Int     @id @default(autoincrement())
  definition  Json
  title       Json
  version     String
}
